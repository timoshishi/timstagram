  "author": "timoshishi",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "predev": "npx prisma generate",
    "setup": "supabase login && aws login && yarn install && yarn prisma:local",
    "test:local": "dotenv -e .env.local jest -- --watch",
    "test:ci": "jest --ci",
    "start": "next start",
    "build-start": "yarn build && yarn start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "yarn build && build-storybook -s public",
    "types:supabase": "supabase gen types typescript --local > ./src/types/database.d.ts",
    "test": "jest --watch",
    "test:e2e:headed": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn cypress open'",
    "setup-coverage": "mv .babel__ .babelrc && rm -rf .nyc_output || true",
    "test:e2e:headless": "start-server-and-test 'yarn dev' 3000 'dotenv -e .env yarn cypress run --record --key'",
    "test:coverage": "yarn test:ci --coverage",
    "posttest:coverage": "yarn report:combined && mv .babelrc .babel__",
    "generate:feature": "node scripts/generate-feature.js $1",
    "generate:api": "node scripts/generate-api.js $1",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:local": "yarn npx prisma format && yarn npx prisma validate && dotenv -e .env.local -- npx prisma migrate",
    "dbpush:local": "dotenv -e .env.local -- npx prisma db push",
    "db:reset": "supabase stop && supabase start",
    "generate:local": "dotenv -e .env.local prisma generate",
    "seed:local": "dotenv -e .env.local -- npx prisma db seed",
    "prisma:local:validate": "npx prisma validate && yarn generate:local",
    "prisma:local": "supabase db reset && npx prisma validate && yarn prisma:local:validate && yarn dbpush:local && yarn seed:local",
    "postprisma:local": "yarn types:supabase",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "yarn mkdir:reports",
    "copy:reports": "cp cypress-coverage/coverage-final.json reports/from-cypress.json && cp jest-coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "yarn copy:reports && mkdir .nyc_output || true",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "yarn combine:reports",
    "report:combined": "npx nyc report --reporter lcov --reporter text --report-dir coverage",
    "prepare": "husky install",
    "pretty-quick": "npx pretty-quick --check src"
  },
